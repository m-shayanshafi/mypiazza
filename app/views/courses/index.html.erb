<div id="courses-table">
    <table>
        <tr>
            <th>Cnum</th>
            <th>Title</th>
            <th>Term</th>
            <th>Start Date</th>
        </tr>
        <% @courses.each do |course| %>
            <tr>
                <td><%= course.cnum %></td>
                <td><%= link_to course.title, course_path(course) %></td>
                <td><%= course.term %></td>
                <td><%= course.start_date %></td>
            </tr>
        <% end %>
    </table>
</div>